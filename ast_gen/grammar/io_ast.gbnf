root ::= inputs outputs "\n"

identifier ::= [a-zA-Z] [a-zA-Z0-9]*
naturalNum ::= [1-9] [0-9]*

width ::= "width:" naturalNum

ports ::= "->" naturalNum "{" width "}"
inputPorts ::= (identifier ports)+
outputPorts ::= (identifier ports)+
inputs ::= "inputs{" inputPorts "}"
outputs ::= "outputs{" outputPorts "}"

